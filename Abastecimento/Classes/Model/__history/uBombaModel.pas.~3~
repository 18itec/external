unit uBombaModel;

interface

type
  TBomba = class
  private
  public
    property  ID:         Integer   read FID        write FID;
    property  NOME:       String    read FNOME      write FNOME;
    property  ID_TANQUE:  Integer   read FID_TANQUE write FID_TANQUE;
    property  STATUS:     Integer   read FSTATUS    write FSTATUS;
  end;


implementation

end.
